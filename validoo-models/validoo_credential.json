{
  "name": "validooCredential",
  "localized_name": "validoo avtal",
  "description": "",
  "alias": [],
  "mixin": ["default_actions", "linked_fields"],
  "defaultSort": "-createdAt",
  "defaultSimilaritySortField": "username",
  "belongs_to": [],
  "calculatedFields": {
    "accessExpired": {
      "tab": "Detaljer",
      "type": "boolean",
      "selfContainedQuery": "SELECT CASE WHEN vc.\"accessExpires\" IS NULL THEN TRUE ELSE vc.\"accessExpires\" < now() + INTERVAL '10 minutes' END FROM \"validooCredentials\" vc WHERE vc.id = ${globals.tableName}.id",
      "is_available_in_reference": true,
      "localized_name": "Access token behöver refresh"
    },
    "refreshExpired": {
      "tab": "Detaljer",
      "type": "boolean",
      "selfContainedQuery": "SELECT CASE WHEN vc.\"refreshExpires\" IS NULL THEN TRUE ELSE vc.\"refreshExpires\" < now() + INTERVAL '10 minutes' END FROM \"validooCredentials\" vc WHERE vc.id = ${globals.tableName}.id",
      "is_available_in_reference": true,
      "localized_name": "Refresh token behöver refresh"
    }
  },
  "fields": {
    "id": {
      "type": "string",
