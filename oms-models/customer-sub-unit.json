    "nrOfContacts": {
      "selfContainedQuery": "COALESCE((SELECT COUNT(\"customerSubUnitContacts\".\"id\") FROM \"customerSubUnitContacts\" WHERE \"customerSubUnitContacts\".\"customerSubUnitId\" = ${globals.tableName}.id and (\"customerSubUnitContacts\".\"owner\" = '${globals.sessionUserOrganizationId}' OR ${globals.isSuper})), 0)",
      "type": "number",
      "localized_name": "Antal kontakter",
      "only_available_in_list": true,
      "roles_for_read": ["supplier", "super"]
    }
  },
