      "required_in_ui": true,
      "is_available_in_reference": true,
      "is_available_in_reference_ui": false
    },
    "currency": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Valuta",
      "max_length": 255,
      "fortnox_equivalent": "Currency",
      "required_in_ui": true,
      "is_available_in_reference": true
