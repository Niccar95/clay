      "should_inform_user": false
    },
    "CUSTOMER_SUB_UNIT_IN_PCS_SERVICE_UPDATED": {
      "localized_name": "customer-sub-unit in PCS service updated",
      "should_inform_service": false,
      "should_inform_user": false
    }
  }
