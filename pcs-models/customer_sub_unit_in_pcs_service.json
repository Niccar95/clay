      "required_in_ui": false,
      "max_length": 255,
      "is_available_in_reference": true
    },
    "invoiceZipCode": {
      "tab": "Fakturering",
      "fortnox_equivalent": "ZipCode",
      "type": "string",
      "localized_name": "Postnummer",
      "required_in_ui": true,
      "max_length": 10,
      "is_available_in_reference": true
    },
    "invoiceCity": {
      "tab": "Fakturering",
      "fortnox_equivalent": "City",
      "type": "string",
      "localized_name": "Stad",
      "required_in_ui": true,
      "is_available_in_reference": true,
      "max_length": 255
    },
    "deliveryCountry": {
      "tab": "Detaljer",
      "include": "restricted_values/country_codes",
      "fortnox_equivalent": "DeliveryCountryCode",
      "localized_name": "Land",
      "required": false,
      "required_in_ui": true,
      "default": "'SE'",
      "is_available_in_reference": true
    },
    "deliveryZipCode": {
      "tab": "Leveransadress",
      "fortnox_equivalent": "DeliveryZipCode",
      "type": "string",
      "localized_name": "Postnummer",
