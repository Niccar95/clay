    "deliveryPhone": {
      "tab": "Leveransadress",
      "fortnox_equivalent": "DeliveryPhone1",
      "type": "string",
      "localized_name": "Leveranstelefon",
      "required": false,
      "max_length": 255,
      "is_available_in_reference": true,
      "roles_for_read": ["super"]
    },
    "deliveryDays": {
      "tab": "Leveransadress",
      "type": "openingHours",
      "localized_name": "Leveransdagar",
      "required": false,
      "is_available_in_reference": true,
      "is_available_in_reference_ui": false
