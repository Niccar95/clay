      "is_available_in_minimum_reference": true,
      "tab": "Detaljer",
      "type": "string",
      "searchable": true,
      "localized_name": "Avdelning/Qvanti användarkonto"
    },
    "customerNumber": {
      "is_available_in_reference": true,
      "is_available_in_minimum_reference": true,
