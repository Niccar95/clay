{
  "name": "article-in-inventory",
  "localized_name": "artikel specifik för inventory-tjänsten",
  "mixin": ["default_actions", "linked_fields"],
  "belongs_to": [],
  "defaultSort": "name",
  "defaultSimilaritySortField": "name",
  "calculatedFields": {},
  "children": {},
  "fields": {
    "id": {
      "type": "string",
      "primary": true,
      "description": "primary key",
      "localized_name": "Identitet",
      "is_available_in_reference": true
    },
    "name": {
      "is_available_in_reference": true,
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "namn"
    },
    "image": {
      "tab": "Detaljer",
      "type": "croppedS3image",
      "localized_name": "Bild",
      "required_in_ui": false,
      "is_available_in_reference": true
    },
    "images": {
      "tab": "Detaljer",
      "type": "imageArray",
      "localized_name": "Bilder",
      "default": "[]",
      "required_in_ui": false,
      "is_available_in_reference": true
    },
    "productCategory": {
      "tab": "Detaljer",
      "type": "pipeString",
      "description": "Specialbenahdlas i transform",
      "localized_name": "Kategori / Typ / Stil",
      "required_in_ui": true,
      "is_available_in_reference": true
    },
    "shortName": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "kort namn",
      "is_available_in_reference": true
    },
    "articleNumber": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "artikelnummer"
    },
    "supplierArticleNumber": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Leverantörens artikelnummer"
    },
    "supplierName": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Leverantörens namn"
    },
    "brandName": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "varumärkets namn"
    },
    "container": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Förpackningstyp",
      "is_available_in_reference": true,
      "is_available_in_reference_ui": true
    },
    "currency": {
      "tab": "Detaljer",
      "type": "currency",
      "localized_name": "Valuta",
      "max_length": 255,
      "fortnox_equivalent": "Currency",
      "is_available_in_reference": true,
      "default": "'SEK'"
    },
    "orderUnit": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Försäljningsenhet",
      "is_available_in_reference": true,
      "is_available_in_reference_ui": true
    },
    "volume": {
      "tab": "Detaljer",
      "type": "number",
      "localized_name": "Volym per styck (liter)",
      "is_available_in_reference": true,
      "is_available_in_reference_ui": false
    },
    "weight": {
      "tab": "Detaljer",
      "type": "number",
      "localized_name": "Vikt per styck (g)",
      "is_available_in_reference": true,
      "is_available_in_reference_ui": false
    },
    "alcoholPercentage": {
      "tab": "Detaljer",
      "type": "number",
      "localized_name": "Alkoholprocent",
      "max_length": 100,
      "step": 0.01,
      "is_available_in_reference": true,
      "is_available_in_reference_ui": false
    },
    "grapes": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Druva"
    },
    "vintage": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "Årgång"
    },
    "GTIN": {
      "tab": "Detaljer",
      "type": "string",
      "localized_name": "artikelnummer",
      "is_available_in_reference": true
    },
    "active": {
      "tab": "Detaljer",
      "type": "boolean",
      "localized_name": "Aktiverad"
    }
  },
  "actions": {},
  "listeners": {
    "ARTICLE_UPDATED": {
      "name": "ARTICLE_UPDATED",
      "reactions": ["updatearticle-in-inventory"]
    },
    "ARTICLE_CREATED": {
      "name": "ARTICLE_CREATED",
      "reactions": ["createarticle-in-inventory"]
    }
  },
  "events": {
    "ARTICLE_IN_INVENTORY_CREATED": {
      "localized_name": "inventory article created",
      "should_inform_service": false,
      "should_inform_user": false
    },
    "ARTICLE_IN_INVENTORY_UPDATED": {
      "localized_name": "inventory article updated",
      "should_inform_service": false,
      "should_inform_user": false
    }
  }
}
